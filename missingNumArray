import java.util.*;

/*Find the Missing number in an sorted array*/
class MissingNumArray {
    public static void main(String[] args) {
       int arr[] = {1, 2, 4, 5, 6};
       int sum=0;
       for(int num: arr){
           sum=num+sum;
       }
       int n = arr[arr.length-1];
       int val = n*(n+1)/2;
       System.out.println(val-sum);
    }
    
}
